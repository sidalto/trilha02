<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/topico32b/webapi/get/:param" method="GET">
        <service class="Webjump\Topico32b\Api\WebApiExampleInterface" method="getParamWebApi" />
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/topico32b/webapi/post" method="POST">
        <service class="Webjump\Topico32b\Api\WebApiExampleInterface" method="postWebApi" />
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
